<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Tela2Activity">
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/bem_vindo"
        android:textAlignment="center"
        android:id="@+id/tituloTela2"
        android:textSize="25dp"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text=""
        android:textAlignment="center"
        android:id="@+id/valorView"
        android:layout_below="@+id/tituloTela2"
        android:textSize="15dp"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/valorView"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="135dp"
        android:id="@+id/btn_processamento"
        android:text="Iniciar Processamento"
        android:textAlignment="center"
        android:onClick="iniciaProcessamento"/>


</RelativeLayout>